ptions)\n        .then((response) => {\n          return response.json();\n        })\n        .then((result) => {\n          if (result.message == \"logout\") {\n            localStorage.removeItem(\"INFO\");\n            this.$store.commit(\"removeUserInfos\");\n            this.$router.push(\"/login\");\n          }\n        });\n    },\n  },\n};\n</script>\n<style scoped>\n.toggle {\n  display: none;\n  flex-direction: column;\n  gap: 2px;\n  cursor: pointer;\n}\n.toggle div {\n  background-color: #5b3cc4;\n  height: 4px;\n  width: 20px;\n}\n\n.navbar {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 25px;\n  min-height: 4em;\n  font-family: \"Lucida Sans\", \"Lucida Sans Regular\", \"Lucida Grande\",\n    \"Lucida Sans Unicode\", Geneva, Verdana, sans-serif;\n  box-shadow: 0px 5px 5px -5px #5b3cc4;\n  z-index: 400;\n  position: fixed;\n  width: 100%;\n  background-color: white;\n  top: 0px;\n}\n\n.menu {\n  display: flex;\n  gap: 2em;\n  align-items: center;\n}\n.menu-items {\n  list-style: none;\n  display: inline-block;\n  letter-spacing: 1px;\n}\n.menu-items a:active {\n  text-decoration: none;\n  color: #1c2820;\n  font-size: 18px;\n}\n.menu-items a {\n  text-decoration: none;\n  color: #5b3cc4;\n  transition: font-size 1s;\n}\n.logo {\n  font-weight: bold;\n  text-transform: uppercase;\n  letter-spacing: 2px;\n  font-size: 21px;\n  color: #5b3cc4;\n  cursor: pointer;\n}\n.right-items {\n  display: flex;\n  align-items: center;\n  gap: 0.5em;\n}\n.login {\n  border: 2px solid #5b3cc4;\n  background-color: white;\n  color: #5b3cc4;\n  outline: none;\n  height: 3em;\n  border-radius: 24px;\n  width: 10em;\n  font-family: \"Lucida Sans\", \"Lucida Sans Regular\", \"Lucida Grande\",\n    \"Lucida Sans Unicode\", Geneva, Verdana, sans-serif;\n  line-height: 0;\n  font-weight: 700;\n  cursor: pointer;\n  transition: 0.5s ease;\n}\n.register {\n  background-color: #5b3cc4;\n  color: white;\n  outline: none;\n  border: non